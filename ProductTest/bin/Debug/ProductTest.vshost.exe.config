<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--LOG设置-->
    <add key="DEBUG" value="1" />
    <!-- NONE  = 0, FATAL = 1, ERROR = 2, WARN  = 3, DEBUG = 4, INFO  = 5, ALL   = 99 -->
    <add key="LogLevel" value="99" />
    <add key="LogTag" value="产品试验" />
    <add key="LogServer" value="127.0.0.1" />
    <add key="LogPort" value="8001" />
    <!-- 数据库类型 -->
    <add key="DatabaseType" value="ACCESS"/>
  </appSettings>
  <connectionStrings>
    <!--<add name="ProductTest" connectionString="Data Source=.\PwrEnvMonitor.db3" providerName="System.Data.SQLite"/>-->
    <add name="ProductTest" connectionString="Provider=Microsoft.Jet.OleDb.4.0;Data Source=.\ProductTest.mdb"/>
  </connectionStrings>
  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/></startup>
</configuration>
